{
  "name": "s3-sink-transactions",
  "config": {
    "connector.class": "io.aiven.kafka.connect.s3.AivenKafkaConnectS3SinkConnector",
    "tasks.max": "1",
    "topics": "transactions",

    "aws.access.key.id": "admin",
    "aws.secret.access.key": "password123",
    "aws.s3.bucket.name": "fraud-raw",
    "aws.region": "us-east-1",
    "aws.endpoint": "http://minio:9000",
    "aws.path.style.access": "true",

    "format.output.type": "jsonl",
    "file.compression": "none",

    "s3.prefix": "event_date=${timestamp:format=yyyy-MM-dd}/hour=${timestamp:format=HH}/",
    "timestamp.extractor": "record",
    "timestamp.field": "timestamp",

    "file.max.records": "50",
    "rotate.interval.ms": "15000",

    "behavior.on.error": "fail",
    "schema.enable": "false",

    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter"
  }
}